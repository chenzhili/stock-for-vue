{
  "name": "stock-for-vue",
  "version": "2.0.1",
  "main": "lib/index.min.js",
  "module": "lib/index.min.js",
  "browser": "lib/index.min.js",
  "license": "MIT",
  "scripts": {
    "build:prod": "node rollup-config/configDir.js && cross-env NODE_ENV=prod babel-node ./rollup-config/index.js",
    "build:watch": "node rollup-config/configDir.js && cross-env NODE_ENV=watch babel-node ./rollup-config/index.js",
    "serve": "vue-cli-service serve --copy",
    "serve:build": "vue-cli-service build",
    "build:test": "node rollup-config/configDir.js && cross-env NODE_ENV=test rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js -w",
    "lint": "eslint  --ext .js,.vue src",
    "styleLint": "stylelint --aei src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "npm run lint"
    ],
    "src/**/*.{html,vue,css,less,scss,styl}": [
      "npm run stylelint"
    ]
  },
  "eslintIgnore": [
    "lib/**",
    "dist/**",
    "rollup.config.js",
    ".eslintrc.js",
    "vue.config.js",
    "node_modules/**",
    "stylelint.config.js",
    "commitlint.config.js",
    "rollup-config/**",
    "example/**"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-dynamic-import-vars": "^1.1.1",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^6.0.0",
    "@vue/cli": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "5.1.2",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.21.0",
    "eslint-plugin-vue": "^7.5.0",
    "generate-source-map": "^0.0.5",
    "husky": "4",
    "lint-staged": "^10.5.4",
    "moment": "^2.29.1",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-nested": "^5.0.3",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^6.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-font": "^1.1.1",
    "rollup-plugin-img": "^1.1.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-require-context": "^1.0.1",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "url-loader": "^4.1.1",
    "vue": "^3.0.5"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "jquery": "^3.6.0"
  },
  "peerDependencies": {}
}
